{
  "version": 2,
  "name": "shipwithatlas",
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "track.html", "use": "@vercel/static" },
    { "src": "style.css", "use": "@vercel/static" },
    { "src": "script.js", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/track", "dest": "/track.html" }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
